{
  "name": "BrainNectar",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run open:node && node bin/www",
    "dev": "webpack && npm run open:webpack && npm run wds",
    "wds": "webpack-dev-server --content-base public/ --hot --inline",
    "open:webpack": "opener http://localhost:8080/webpack-dev-server/",
    "open:node": "opener http://localhost:3000/"
  },
  "dependencies": {
    "body-parser": "~1.12.4",
    "cookie-parser": "~1.3.5",
    "node-event-emitter": "0.0.1",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "morgan": "~1.5.3",
    "serve-favicon": "~2.2.1"
  },
  "devDependencies": {
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "opener": "^1.4.1",
    "parallelshell": "^1.1.1",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
